{
  "name": "@jepz20/conman",
  "version": "0.0.4",
  "description": "Configuration manager that supports ttl and plugabble sources",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "precommit": "lint-staged",
    "prettify": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a1877d23a24042de10fa25296f519a3c681712f2",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "jsonfile": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^7.0.0",
    "prettier": "1.18.2",
    "prettier-eslint-cli": "4.7.1",
    "eslint-plugin-prettier": "2.7.0",
    "lint-staged": "^7.3.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ]
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "src/**/*.*": [
      "prettier --write",
      "git add"
    ]
  }
}
